{
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["es5", "dom", "ES2015"],
    "types": ["cypress", "node", "@testing-library/cypress", "zod"],
    "baseUrl": "../",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["**/*.ts"]
}
